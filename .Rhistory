complete <- function(directory, id=1:332) {
files <- list.files(directory)
nobs = c()
for (file in files[id]) {
relpath = paste(directory, file, sep='/')
data = read.csv(relpath)
mask = !is.na(data)
data_complete = data[mask]
nobs = c(nobs, length(mask))
}
data.frame(id, nobs)
}
